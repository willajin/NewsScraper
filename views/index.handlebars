<div class="container">
    <div class="row">
        <h1>News Scraper</h1>
    </div>

    <div class="row">
        <div class="col s6">
            <div id="articles">
                {{#each dbArticle}}
                <p data-id={{_id}}>{{headline}}</p>
                <p>{{summary}}</p>
                <p>{{url}}</p>
                {{/each}}
            </div>
        </div>
        <div class="col s6">
            <div id="notes"></div>
        </div>
    </div>

</div> <!-- end container div -->